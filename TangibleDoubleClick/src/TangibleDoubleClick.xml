<?xml version="1.0" encoding="UTF-8"?><component xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.rustle.no-ip.info/satin/eclipse/schema/SatinComponentSchema.xsd">
	
	<!--Url to the icon that represent the component in the editor -->
	<icon>http://satin.codemill.se:81/compicons/43105316-751e-4fa4-9353-05300a4cb6f6.png</icon>
	
	<!--Unique identifier for the component -->
	<name>TangibleDoubleClick</name>
	
	<!--Description of the component -->
	<info><![CDATA[Work as TangibleButton but add a double click detection. When the device is pressed, the component fires the click event, and if it is pressed again during the selected delay, a double_click event is fired instead]]></info>
	
	<!--Configuration parameters -->
	<config>
		<param default="TriggerCube" name="device_label" type="String"/>
		<param default="0.1" name="delay" type="Int"/>
	</config>
	
	<!--Required interface: a number of push or pull ports -->
	<requires>
		<port kind="Push" name="click"/>
		<port kind="Push" name="double_click"/>
	</requires>
	
	<!--Provided interface: a number of push or pull ports -->
	<provides>
	</provides>
	
